				<key>script</key>
				<string>#!/bin/bash
long=`sw_vers -productVersion`
short=${long:0:4}
if [ "$short" == "10.4" ]; then
    lookupd -flushcache
elif [ "$short" == "10.5" ] || [ "$short" == "10.6" ]; then
    dscacheutil -flushcache
elif [ "$short" == "10.7" ] || [ "$short" == "10.8" ]; then
    sudo killall -HUP mDNSResponder
fi</string>
